{
  "name": "react-boilerplate-ie8",
  "version": "1.0.0",
  "description": "支持IE8的react脚手架",
  "scripts": {
    "copy": "copyfiles -f ./favicon.ico ./shim.js ./fetch-ie8.js ./placeholders-ie8.js ./dist",
    "start": "node server.js --env=dev",
    "start:mock": "node server.js --env=dev --mock",
    "ie8": "node server.js --env=dev-ie8",
    "ie8:mock": "node server.js --env=dev-ie8 --mock",
    "dist": "webpack --env=dist",
    "mock": "json-server --watch ./mock/db.json",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false"
