{
  'parser': 'babel-eslint',
  'plugins': [
    'react'
  ],
  'parserOptions': {
    'ecmaVersion': 6,
    'sourceType': 'module',
    'ecmaFeatures': {
      'jsx': true
    }
  },
  extends: [
    'standard',
    'standard-react'
  ],
  'env': {
    'browser': true,
    'amd': true,
    'es6': true,
    'node': true,
    'mocha': true
  },
  'rules': {
    # 'comma-dangle': 1,
    # 'quotes': [ 1, 'single' ],
    # 'no-undef': 1,
    # 'global-strict': 0,
    # 'no-extra-semi': 1,
    # 'no-underscore-dangle': 0,
    # 'no-console': 1,
    # 'no-unused-vars': 1,
    # 'no-trailing-spaces': [1, { 'skipBlankLines': true }],
    # 'no-unreachable': 1,
    # 'no-alert': 0,
    'arrow-parens':0,
    'jsx-quotes':[2,'prefer-double'],
    'space-before-function-paren': 0,
    'no-debugger': 1,
    'react/jsx-space-before-closing': 0
  }
}
